{
  "name": "flipboxdigital/relay",
  "description": "Relay",
  "version": "2.1.0",
  "keywords": [
    "relay",
    "flipbox",
    "psr-7"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "flipboxdigital/skeleton": "^2.0.1",
    "psr/log": "^1.0.1",
    "relay/relay": "^1.0",
    "zendframework/zend-diactoros": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Relay\\": "src/"
    },
    "files": [
      "src/Helpers/RelayHelper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Relay\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}